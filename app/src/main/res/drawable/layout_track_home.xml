<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true">
        <shape>
            <corners android:radius="@dimen/size30"/>
            <solid android:color="#272727"/>
            <size android:height="@dimen/size44" android:width="@dimen/size95"/>
            <stroke android:width="@dimen/size5" android:color="#191919"/>
            <padding/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <corners android:radius="@dimen/size30"/>
            <solid android:color="#272727"/>
            <size android:height="@dimen/size44" android:width="@dimen/size95"/>
            <stroke android:width="@dimen/size5" android:color="#191919"/>
            <padding/>
        </shape>
    </item>
</selector>
