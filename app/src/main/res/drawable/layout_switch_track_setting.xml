<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <size android:height="@dimen/size20" android:width="@dimen/size30"/>
            <solid android:color="#1fd686"/>
            <corners android:radius="@dimen/size100"/>
            <stroke android:width="@dimen/size1" android:color="#1fd686"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <size android:height="@dimen/size20" android:width="@dimen/size30"/>
            <solid android:color="@color/main"/>
            <corners android:radius="@dimen/size100"/>
            <stroke android:width="@dimen/size1" android:color="#00ffffff"/>
        </shape>
    </item>
</selector>
